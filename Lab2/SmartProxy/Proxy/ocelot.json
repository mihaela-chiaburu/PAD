{
  "Routes": [
    {
      "UpstreamHttpMethod": [ "Get" ],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      },
      "FileCacheOptions": {
        "TtlSeconds": 5,
        "Region": "movieCacheRegion",
        "Header": "OC-Caching-Control",
        "EnableContentHashing": false
      },
      "UpstreamPathTemplate": "/{url}",
      "DownstreamPathTemplate": "/{url}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "movieapi-instance1.railway.internal",
          "Port": 443
        },
        {
          "Host": "movieapi-instance2.railway.internal",
          "Port": 443
        }
      ]
    },
    {
      "UpstreamHttpMethod": [ "Put", "Post", "Delete" ],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      },
      "UpstreamPathTemplate": "/{url}",
      "DownstreamPathTemplate": "/{url}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "movieapi-instance1.railway.internal",
          "Port": 443
        },
        {
          "Host": "movieapi-instance2.railway.internal",
          "Port": 443
        }
      ]
    }
  ]
}